<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="signup" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="830.0"
	prefWidth="1440.0" xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.SignUpController">

	<children>
		<AnchorPane minHeight="0.0" minWidth="0.0"
			prefHeight="830.0" prefWidth="1440.0">
			<children>
				<Button fx:id="signUpBtn" alignment="CENTER" layoutX="940.0"
					layoutY="560.0" mnemonicParsing="false" onAction="#signup"
					opacity="0.96" prefHeight="37.0" prefWidth="106.0" text="Signup"
					textAlignment="CENTER">
					<font>
						<Font size="21.0" />
					</font>
					<effect>
						<DropShadow />
					</effect>
				</Button>
				<Label fx:id="signUpLbl" alignment="CENTER" layoutX="515.0"
					layoutY="150.0" prefHeight="32.0" prefWidth="410.0" text="Signup">
					<font>
						<Font size="21.0" />
					</font>
				</Label>
				<PasswordField fx:id="confirmPw" layoutX="570.0"
					layoutY="450.0" prefHeight="42.0" prefWidth="320.0"
					promptText="Confirm password">
					<font>
						<Font size="21.0" />
					</font>
				</PasswordField>
				<PasswordField fx:id="newUsrPw" layoutX="570.0"
					layoutY="365.0" prefHeight="42.0" prefWidth="320.0"
					promptText="Password">
					<font>
						<Font size="21.0" />
					</font>
				</PasswordField>
				<TextField fx:id="newUsrName" layoutX="570.0"
					layoutY="285.0" prefHeight="42.0" prefWidth="320.0"
					promptText="Username">
					<font>
						<Font size="21.0" />
					</font>
				</TextField>
				<Button fx:id="back" alignment="CENTER" layoutX="396.0"
					layoutY="560.0" mnemonicParsing="false" onAction="#returnLogin"
					opacity="0.96" prefHeight="37.0" prefWidth="106.0" text="Back"
					textAlignment="CENTER">
					<font>
						<Font size="21.0" />
					</font>
					<effect>
						<DropShadow />
					</effect>
				</Button>
			</children>
		</AnchorPane>
	</children>
</Pane>
