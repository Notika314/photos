<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="830.0"
	prefWidth="1440.0" stylesheets="@albumTab.css"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.AlbumController">
	<children>
		<HBox prefHeight="830.0" prefWidth="1440.0">
			<children>
				<ListView fx:id="listView" minWidth="480.0"
					prefHeight="830.0" prefWidth="480.0" />
				<VBox prefHeight="830.0" prefWidth="960.0">
					<children>
						<AnchorPane prefHeight="315.0" prefWidth="960.0" />
						<TabPane prefHeight="515.0" prefWidth="960.0"
							tabClosingPolicy="UNAVAILABLE">
							<tabs>
								<Tab closable="false" style="-fx-background-radius: 0;"
									text="Create">
									<content>
										<AnchorPane minHeight="0.0" minWidth="0.0"
											prefHeight="515.0" prefWidth="960.0">
											<children>
												<TextField fx:id="createField" alignment="CENTER"
													layoutX="320.0" layoutY="120.0" prefHeight="42.0"
													prefWidth="320.0" promptText="Album Name">
													<font>
														<Font size="20.0" />
													</font>
												</TextField>
												<Button fx:id="createBtn" depthTest="ENABLE"
													layoutX="420.0" layoutY="248.0" mnemonicParsing="false"
													onAction="#create" prefHeight="40.0" prefWidth="120.0"
													style="-fx-background-radius: 0;" text="Create">
													<font>
														<Font size="19.0" />
													</font>
													<effect>
														<DropShadow />
													</effect>
												</Button>
											</children>
										</AnchorPane>
									</content>
								</Tab>
								<Tab closable="false" style="-fx-background-radius: 0;"
									text="Rename">
									<content>
										<AnchorPane minHeight="0.0" minWidth="0.0"
											prefHeight="515.0" prefWidth="960.0">
											<children>
												<TextField fx:id="renameField" alignment="CENTER"
													layoutX="320.0" layoutY="120.0" prefHeight="42.0"
													prefWidth="320.0" promptText="Album Name">
													<font>
														<Font size="20.0" />
													</font>
												</TextField>
												<Button fx:id="renameBtn" depthTest="ENABLE"
													layoutX="420.0" layoutY="250.0" mnemonicParsing="false"
													onAction="#rename" prefHeight="40.0" prefWidth="120.0"
													style="-fx-background-radius: 0;" text="Rename">
													<font>
														<Font size="19.0" />
													</font>
													<effect>
														<DropShadow />
													</effect>
												</Button>
											</children>
										</AnchorPane>
									</content>
								</Tab>
								<Tab closable="false" style="-fx-background-radius: 0;"
									text="Delete">
									<content>
										<AnchorPane minHeight="0.0" minWidth="0.0"
											prefHeight="180.0" prefWidth="200.0">
											<children>
												<Button fx:id="deleteBtn" depthTest="ENABLE"
													layoutX="420.0" layoutY="110.0" mnemonicParsing="false"
													onAction="#delete" prefHeight="40.0" prefWidth="120.0"
													style="-fx-background-radius: 0;" text="Delete">
													<font>
														<Font size="19.0" />
													</font>
													<effect>
														<DropShadow />
													</effect>
												</Button>
											</children>
										</AnchorPane>
									</content>
								</Tab>
								<Tab closable="false" style="-fx-background-radius: 0;"
									text="Search">
									<content>
										<AnchorPane minHeight="0.0" minWidth="0.0"
											prefHeight="515.0" prefWidth="960.0">
											<children>
												<TabPane prefHeight="472.0" prefWidth="960.0"
													tabClosingPolicy="UNAVAILABLE">
													<tabs>
														<Tab
															style="-fx-background-radius: 0; -fx-pref-width: 468;"
															text="Time">
															<content>
																<AnchorPane minHeight="0.0" minWidth="0.0"
																	prefHeight="180.0" prefWidth="200.0">
																	<children>
																		<TextField fx:id="startField"
																			alignment="CENTER" layoutX="96.0" layoutY="113.0"
																			prefHeight="42.0" prefWidth="320.0"
																			promptText="Start Date">
																			<font>
																				<Font size="20.0" />
																			</font>
																		</TextField>
																		<TextField fx:id="endField"
																			alignment="CENTER" layoutX="555.0" layoutY="113.0"
																			prefHeight="42.0" prefWidth="320.0"
																			promptText="End Date">
																			<font>
																				<Font size="20.0" />
																			</font>
																		</TextField>
																		<Button fx:id="searchTimeBtn"
																			depthTest="ENABLE" layoutX="420.0" layoutY="239.0"
																			mnemonicParsing="false" onAction="#searchTime"
																			prefHeight="40.0" prefWidth="120.0"
																			style="-fx-background-radius: 0;" text="Search">
																			<font>
																				<Font size="19.0" />
																			</font>
																			<effect>
																				<DropShadow />
																			</effect>
																		</Button>
																	</children>
																</AnchorPane>
															</content>
														</Tab>
														<Tab
															style="-fx-background-radius: 0; -fx-pref-width: 468;"
															text="Tags">
															<content>
																<AnchorPane minHeight="0.0" minWidth="0.0"
																	prefHeight="200.0" prefWidth="200.0">
																	<children>
																		<TextField fx:id="createField1"
																			alignment="CENTER" layoutX="46.0" layoutY="72.0"
																			prefHeight="42.0" prefWidth="320.0"
																			promptText="Type 1 (Required)">
																			<font>
																				<Font size="20.0" />
																			</font>
																		</TextField>
																		<TextField fx:id="createField11"
																			alignment="CENTER" layoutX="46.0" layoutY="160.0"
																			prefHeight="42.0" prefWidth="320.0"
																			promptText="Value 1 (Required)">
																			<font>
																				<Font size="20.0" />
																			</font>
																		</TextField>
																		<TextField fx:id="createField12"
																			alignment="CENTER" layoutX="589.0" layoutY="72.0"
																			prefHeight="42.0" prefWidth="320.0"
																			promptText="Type 2 (Optional)">
																			<font>
																				<Font size="20.0" />
																			</font>
																		</TextField>
																		<TextField fx:id="createField111"
																			alignment="CENTER" layoutX="589.0" layoutY="160.0"
																			prefHeight="42.0" prefWidth="320.0"
																			promptText="Value 2 (Optional)">
																			<font>
																				<Font size="20.0" />
																			</font>
																		</TextField>
																		<Button fx:id="andBtn" layoutX="445.0"
																			layoutY="79.0" mnemonicParsing="false"
																			prefHeight="35.0" prefWidth="70.0"
																			style="-fx-background-radius: 0;" text="AND" />
																		<Button fx:id="orBtn" layoutX="445.0"
																			layoutY="146.0" mnemonicParsing="false"
																			prefHeight="35.0" prefWidth="70.0"
																			style="-fx-background-radius: 0;" text="OR" />
																		<Button fx:id="searchTagBtn" depthTest="ENABLE"
																			layoutX="420.0" layoutY="282.0"
																			mnemonicParsing="false" onAction="#searchTag"
																			prefHeight="40.0" prefWidth="120.0"
																			style="-fx-background-radius: 0;" text="Search">
																			<font>
																				<Font size="19.0" />
																			</font>
																			<effect>
																				<DropShadow />
																			</effect>
																		</Button>
																	</children>
																</AnchorPane>
															</content>
														</Tab>
													</tabs>
												</TabPane>
											</children>
										</AnchorPane>
									</content>
								</Tab>
							</tabs>
						</TabPane>
					</children>
				</VBox>
			</children>
		</HBox>
	</children>
</Pane>
